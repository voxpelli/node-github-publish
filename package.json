{
  "name": "github-publish",
  "version": "2.0.0",
  "license": "MIT",
  "description": "Publishes a file to a repository through the GitHub Contents API",
  "author": "Pelle Wessman <pelle@kodfabrik.se> (http://kodfabrik.se/)",
  "homepage": "https://github.com/voxpelli/node-github-publish",
  "repository": {
    "type": "git",
    "url": "git://github.com/voxpelli/node-github-publish.git"
  },
  "main": "index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "node .",
    "mocha": "NODE_ENV=test istanbul cover _mocha -- -u exports -R spec test/**/*.spec.js",
    "dependency-check": "JS_FILES=\"*.js test/*.js\" && dependency-check . $JS_FILES && dependency-check . $JS_FILES --unused --no-dev",
    "test": "installed-check -e && eslint . && npm run dependency-check && npm run mocha",
    "prepush": "npm test"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "dependency-check": "^2.5.2",
    "eslint": "^4.14.0",
    "eslint-config-semistandard": "^12.0.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "installed-check": "^2.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.1.0",
    "nock": "^9.0.0"
  },
  "dependencies": {
    "node-fetch": "^1.3.0",
    "verror": "^1.6.0"
  }
}
